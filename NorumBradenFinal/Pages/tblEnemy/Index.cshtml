@page
@model NorumBradenFinal.Pages.tblEnemy.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TblEnemy[0].Enemyhealth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TblEnemy[0].Enemyattack)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.TblEnemy) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Enemyhealth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enemyattack)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Enemyid">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Enemyid">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Enemyid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
